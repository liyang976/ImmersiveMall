.user-profile {
    width: 750rpx;
    height: 300rpx;
    display: flex;
    padding: 0 30rpx;
    align-items: center;
    background-color: #f2cac3;
    color: #fff;
}

.user-profile image {
    width: 200rpx;
    height: 200rpx;
    border-radius: 100rpx;
    margin-right: 30rpx;
}

.icon-list .list-title {
    font-size: 30rpx;
    font-weight: bold;
    padding: 10rpx 55rpx;
}